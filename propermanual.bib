@article{dietrich_reason-based_2016,
	title = {Reason-based choice and context-dependence: an explanatory framework},
	volume = {32},
	issn = {1474-0028},
	doi = {10.1017/S0266267115000474},
	abstract = {Abstract:We introduce a ‘reason-based’ framework for explaining and predicting individual choices. The key idea is that a decision-maker focuses on some but not all properties of the options and chooses an option whose ‘motivationally salient’ properties he/she most prefers. Reason-based explanations can capture two kinds of context-dependent choice: (i) the motivationally salient properties may vary across choice contexts, and (ii) they may include ‘context-related’ properties, not just ‘intrinsic’ properties of the options. Our framework allows us to explain boundedly rational and sophisticated choice behaviour. Since properties can be recombined in new ways, it also offers resources for predicting choices in unobserved contexts.},
	number = {2},
